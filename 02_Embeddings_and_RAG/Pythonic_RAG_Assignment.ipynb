        "def parse_pdf(file_path):\n",
        "    \"\"\"Extracts text from a PDF using PyMuPDF.\"\"\"\n",
        "    document = fitz.open(file_path)\n",
        "    text = \"\"\n",
        "    for page_num in range(len(document)):\n",
        "        page = document.load_page(page_num)\n",
        "        text += page.get_text(\"text\")  # Ensures better text extraction\n",
        "    return text\n",
        "\n",
        "def parse_file(file_path):\n",
        "    \"\"\"Handles file parsing for PDFs and text files.\"\"\"\n",
        "    if file_path.endswith('.pdf'):\n",
        "        return parse_pdf(file_path)\n",
        "    else:\n",
        "        with open(file_path, 'r', encoding=\"utf-8\") as file:\n",
        "            return file.read()\n",
        "\n",
        "# Example usage\n",
        "file_path = 'example.pdf'\n",
        "parsed_text = parse_file(file_path)\n",
        "\n",
        "# Debug: Print first 500 characters to ensure text is extracted\n",
        "print(f\"Extracted Text Preview:\\n{parsed_text[:500]}\")\n",
        "\n",
        "# Use RecursiveCharacterTextSplitter to avoid splitting text into individual characters\n",
        "text_splitterPDF = RecursiveCharacterTextSplitter(chunk_size=500, chunk_overlap=50)\n",
        "split_documentsPDF = text_splitterPDF.split_text(parsed_text)\n",
        "\n",
        "print(f\"\\nTotal Chunks: {len(split_documentsPDF)}\")\n",
        "print(f\"First Chunk:\\n{split_documentsPDF[0]}\")\n"
